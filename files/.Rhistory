library(ggplot2)
ggplot(tourism, aes(x = date)) +geom_line(aes(y=trend), colour="red") + geom_line(aes(y=people), colour="green")
plot(tourism$date, tourism$people / 1000, type = "l", ylab = "Visitors (thousands)", xlab = "Months")
